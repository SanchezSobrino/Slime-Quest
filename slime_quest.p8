pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- slime quest
-- by sanchezsobrino

debug_enabled = false
current_room = {2,1}

-- player stuff
function make_player()
 local pl = {}
 pl.spr   = 1
 pl.x     = 56
 pl.y     = 56
 pl.spd   = 1
 pl.mv    = false
 pl.hp    = 10
 pl.dirx  = 0  -- 0 r, 1 l
 pl.diry  = 0  -- 0 d, 1 u
 pl.flip  = false
 pl.anim_d= 1
 pl.c     = 0
 pl.c2    = 0
 
 return pl
end

function handle_input()
 local tmp_x = pl.x
 local tmp_y = pl.y
 
 if (btn(0)) then
  tmp_x -= pl.spd
  
  if (pl.dirx == 0) then
   pl.flip = true
  end
  pl.dirx = 1
  
  if (is_solid(tmp_x,tmp_y+2) or 
      is_solid(tmp_x,tmp_y+7)) then
   tmp_x = pl.x
  else
   pl.x = tmp_x
  end
  
  pl.mv = true
 end
 
 if (btn(1)) then 
  tmp_x += pl.spd
  
  if (pl.dirx == 1) then
   pl.flip = false
  end
  pl.dirx = 0
  
  if (is_solid(tmp_x+7,tmp_y+2) or 
      is_solid(tmp_x+7,tmp_y+7)) then
   tmp_x = pl.x
  else
   pl.x = tmp_x
  end
  
  pl.mv = true
 end
 
 if (btn(2)) then 
  tmp_y -= pl.spd
  pl.diry = 1
  
  if (is_solid(tmp_x,tmp_y+2) or 
      is_solid(tmp_x+7,tmp_y+2)) then
   tmp_y = pl.y
  else
   pl.y = tmp_y
  end
  
  pl.mv = true
 end
 
 if (btn(3)) then
  tmp_y += pl.spd
  pl.diry = 0
  
  if (is_solid(tmp_x,tmp_y+7) or 
      is_solid(tmp_x+7,tmp_y+7)) then
   tmp_y = pl.y
  else
   pl.y = tmp_y
  end
  
  pl.mv = true
 end
end

function handle_room_change()
 if (pl.x > 120) then
  change_room(1,0)
 end
 
 if (pl.x < 0) then
  change_room(-1,0)
 end
 
 if (pl.y > 120) then
  change_room(0,1)
 end
 
 if (pl.y < 0) then
  change_room(0,-1)
 end
end

function update_player()
 pl.c = interval(pl.c, pl.anim_d,
     function()
      pl.spr = 1 + (pl.spr % 3)
     end)
 pl.mv = false
 
 handle_input()
 handle_room_change()

 if (not pl.mv) then
  pl.spr = 1
 end
end

-- map stuff
function draw_room(r)
 map(r[1]*16,r[2]*16,0,0,16,16)
 
 update_water(r)
end

function change_room(x,y)
 current_room[1] += x
 current_room[2] += y
 local r = current_room
 local plx, ply = pl.x, pl.y
 
 pl.x += x*8
 pl.y += y*8
 
 for i=0,16 do
  if (x==1) then
   map(r[1]*16-(16-i),r[2]*16,0,0,16,16)
   map(r[1]*16,r[2]*16,(128-(i*8)),0,i,16)
   plx = 0
  elseif (y==1) then
   map(r[1]*16,r[2]*16-(16-i),0,0,16,16)
   map(r[1]*16,r[2]*16,0,(128-(i*8)),16,i)
   ply = 0
  elseif (x==-1) then
   map((r[1]+1)*16,r[2]*16,x*8,0,16-i,16)
   map(r[1]*16+(16-i),r[2]*16,0,0,16,16)
   plx = 120
  elseif (y==-1) then
   map(r[1]*16,(r[2]+1)*16,0,i*8,16,16-i)
   map(r[1]*16,r[2]*16+(16-i),0,0,16,16)
   ply = 120
  end

  pl.x += (-x)*8
  pl.y += (-y)*8
  spr(pl.spr,pl.x,pl.y,1,1,pl.flip)

  flip()
 end
 
 pl.x = plx
 pl.y = ply
end

function get_spr(x,y)
 local r = current_room
 return mget(r[1]*16+flr(x/8),
             r[2]*16+flr(y/8))
end

function is_solid(x,y)
 local s = get_spr(x,y)
 return fget(s,0)
end

function update_water(r)
 local t = flr(time())
 local rx = r[1]*16
 local rx_off = rx+16
 local ry = r[2]*16
 local ry_off = ry+16
 
 for x=rx,rx_off do
  for y=ry,ry_off do
   local s = mget(x,y)
   if ((s == 52) or (s == 53)) then
    mset(x,y,(t%2)+52)
   end
  end
 end
end

-- timers
function make_timer(trigger_s,c)
 t = {}
 t.s = 0
 t.real = 0
 t.trigger = trigger_s
 t.callback = c

 add(timers,t)
 return t
end

function update_timers()
 for i=1,count(timers) do
  timers[i].s += 1
  
  if (t.s > 30) then
   timers[i].s = 0
   timers[i].real += 1
   
   if (timers[i].real >= timers[i].trigger) then
    timers[i].callback()
   end
  end
 end
end

function interval(c,d,callback)
	if (c < d) then
  c += 1
 else
  c = 0
  callback()
 end
 
 return c
end

-- pico-8 callbacks
function _init()
 pl = make_player()
 timers = {}
end

function _update()
 update_player()
 update_timers()
end

function _draw()
 draw_room(current_room)
 spr(pl.spr,pl.x,pl.y,1,1,pl.flip)
 
 if (debug_enabled) then 
  debug()
 end
end

-- some debug information
-- only callable when
-- debug_enabled = true
function debug()
 local r = current_room
 
 rectfill(0,112,128,128,0)
 print(pl.x..","..pl.y,2,114,7)
 --print(r[1]..","..r[2],2,122,7)
 print(pl.c2,2,122,7)
end

